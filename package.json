{
  "name": "sparq-figma-mcp-dev",
  "description": "SPARQ Figma MCP - Local Development Version",
  "version": "1.0.0",
  "module": "dist/server.js",
  "main": "dist/server.js",
  "bin": {
    "sparq-figma-mcp-dev": "dist/server.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "start": "bun run dist/server.js",
    "socket": "bun run src/socket.ts",
    "setup": "./scripts/setup.sh",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "bun run build:watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "package:plugin": "node scripts/package-plugin.js",
    "pub:release": "bun run build && npm publish"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.0-beta.3",
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "bun-types": "^1.2.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "uuid": "latest",
    "ws": "latest",
    "zod": "latest"
  }
}